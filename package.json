{
	"name": "llume",
	"version": "0.1.17",
	"description": "AI agent framework for executing tasks with LLMs and tools",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"type": "commonjs",
	"keywords": ["ai", "agent", "llm", "tools", "llume"],
	"author": "uxname <uxname@gmail.com>",
	"license": "MIT",
	"scripts": {
		"start": "bun run src/main.ts",
		"test": "vitest run --config vitest.config.ts",
		"test:watch": "vitest watch --config vitest.config.ts",
		"ts:check": "bunx tsc --noEmit",
		"check": "bun run ts:check && bun run lint",
		"lint": "bunx biome check .",
		"lint:fix": "bunx biome check --write .",
		"prebuild": "bun run check && bunx rimraf dist",
		"build": "bunx tsc -p tsconfig.build.json",
		"prepublishOnly": "bun run build && npm pack --dry-run"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@types/bun": "latest",
		"copyfiles": "^2.4.1",
		"dotenv": "^16.4.7",
		"jiti": "^2.4.2",
		"rimraf": "^6.0.1",
		"typescript": "^5.8.3",
		"vitest": "^3.1.1"
	},
	"peerDependencies": {},
	"private": false,
	"dependencies": {
		"axios": "^1.8.4",
		"handlebars": "^4.7.8",
		"picocolors": "^1.1.1",
		"zod": "^3.24.2",
		"zod-to-json-schema": "^3.24.5"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/uxname/llume.git"
	},
	"files": ["dist/**/*", "README.md", "LICENSE"]
}
